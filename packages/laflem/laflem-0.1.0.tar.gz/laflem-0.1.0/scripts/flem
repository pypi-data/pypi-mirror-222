#!/usr/bin/env python
import os
import sys

from flem import Flem

if __name__ == '__main__':
  #print("Hello, World!")

  #file_path = os.path.join(sys.prefix, 'configs/flem/base.yml')
  #with open(file_path, 'r') as f:
  #  print(f.read())

  f = Flem()
  args = f.parser.parse_args()
  
  if args.collection:
    collection = f.get_collection(args.collection)
    if args.module:
      module = collection.get_module(args.module)

      module.run(args)
      sys.exit(0)
